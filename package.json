  "license": "Apache-2.0",
  "dependencies": {
    "hyphenize": "0.0.2",
    "ini": "^1.3.4",
    "js-yaml": "^3.5.5",
    "q": "^1.4.1",
    "semver": "^5.1.0",
    "yeoman-generator": "^0.22.5"
  },
  "devDependencies": {
    "eslint": "^3.3.1",
    "eslint-config-openstack": "^4.0.1",
    "istanbul": "^0.4.2",
    "jasmine": "^2.4.1",
    "jsdoc": "^3.4.0",
    "karma": "^0.13.22",
    "karma-htmlfile-reporter": "^0.2.2",
    "mem-fs": "^1.1.2",
    "mem-fs-editor": "^2.2.0",
    "nsp": "^2.3.0",
    "yeoman-assert": "^2.1.2",
